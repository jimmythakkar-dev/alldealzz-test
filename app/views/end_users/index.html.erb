
<div class="row title_header">
	<div class="col-md-8">
	<%= title_header(@end_user, "End Users") %>
	</div>
</div>
<ol class="breadcrumb">
  <%= end_user_breadcrumb_list(nil, 'End Users') %>
</ol>

<div class="row title_header">
  <div class="col-md-12">
    <h3>Total Users: <%= @end_users.total_count %>
    </h3>
  </div>
</div>

<div class="search">
	<%= render 'end_users/listing/search', end_users: @end_users %>
</div>

<div class="end_users_list">
  <%= render 'end_users/listing/list', end_users: @end_users %>
</div>

<script type="text/javascript">
$(document).ready(function () {
	$('input#search_text').keyup(function(e) {
      if(e.keyCode == 13) { search_request(); }
    });
  });

  function search_request() {
    $.ajax({
      type: "GET",
      dataType: "script",
      url: "end_users",
      data: { search:  { text: $('input#search_text').val() } }
    });
  }
</script>