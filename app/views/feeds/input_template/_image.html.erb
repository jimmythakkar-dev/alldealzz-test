<div class="row">
  <div class="col-md-6"> 
    <div class="form-group">
      <%= f.label :photo,'Photo',class: "col-sm-3 control-label" %>
      <div class="col-sm-8">
        <%- if f.object.photo.present? && f.object.photo.url.present? %>
          <img src='<%= f.object.photo.url %>' width='100' height='100'> 
        <% end -%>  
        <%= f.file_field :photo %>
      </div>  
    </div>
    <div class="form-group">
      <%= f.label :feed_detail_images, class: "col-sm-3 control-label" %>
      <div class="controls">
        <%= file_field_tag "images[]", type: :file, multiple: true %>
      </div>
    </div>
  </div>  
</div>